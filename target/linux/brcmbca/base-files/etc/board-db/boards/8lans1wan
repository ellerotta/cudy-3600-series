
config board 'board'
	# Hardware specs
	option hasEthWan '1'
	# DSL PON WIFI SF/Runner Ethernet
	option ethernetPortNames 'LAN1 LAN2 LAN3 LAN4 LAN5 LAN6 LAN7 LAN8 WAN'
	option ethernetPortOrder 'eth1 eth2 eth3 eth4 eth5 eth6 eth7 eth8 eth0'
	option ethernetLanPorts 'eth1 eth2 eth3 eth4 eth5 eth6 eth7 eth8 '
	option ethernetWanPort 'eth0'
